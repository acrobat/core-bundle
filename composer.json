{
    "name": "symfony-cmf/core-bundle",
    "type": "symfony-bundle",
    "description": "Symfony CMF Core Bundle",
    "keywords": [
        "Symfony CMF"
    ],
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/CoreBundle/contributors"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/framework-bundle": "^5.4 || ^6.3",
        "symfony/security-core": "^5.4 || ^6.3"
    },
    "require-dev": {
        "jackalope/jackalope-doctrine-dbal": "^1.3",
        "symfony/security-bundle": "^5.4 || ^6.3",
        "symfony/phpunit-bridge": "^6.3",
        "mockery/mockery": "^1.4.1",
        "symfony-cmf/routing-bundle": "^2.1.0 || ^3.0",
        "symfony-cmf/testing": "^4.0.0",
        "doctrine/dbal": "^2.5",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/phpcr-odm": "^1.4|^2.0 ",
        "doctrine/phpcr-bundle": "^2.3.0",
        "symfony/browser-kit": "^5.4 || ^6.3",
        "symfony/form": "^5.4 || ^6.3",
        "symfony/monolog-bridge": "^5.4 || ^6.3",
        "symfony/monolog-bundle": "^3.0",
        "symfony/templating": "^5.4 || ^6.3",
        "symfony/translation": "^5.4 || ^6.3",
        "symfony/twig-bundle": "^5.4 || ^6.3",
        "symfony/validator": "^5.4 || ^6.3",
        "symfony/yaml": "^5.4 || ^6.3",
        "symfony/asset": "^5.4 || ^6.3"
    },
    "suggest": {
        "symfony/twig-bundle": "To get access to the CMF twig extension (^4.4 || ^5.0)",
        "doctrine/phpcr-bundle": "To be able to use the CMF twig extension (^1.0)",
        "doctrine/phpcr-odm": "To be able to use the CMF twig extension (^1.0)",
        "symfony/security-bundle": "To be able to use the publish workflow system (^4.4 || ^5.0)",
        "symfony-cmf/routing": "To be able to use the CMF twig extension functions cmf_prev_linkable/cmf_next_linkable (^2.1)",
        "symfony-cmf/routing-bundle": "To be able to enable the publish_workflow_listener (^2.1)",
        "symfony-cmf/sonata-admin-integration-bundle": "To provide an admin interface for the PHPCR ODM documents."
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\CoreBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\CoreBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "config" : {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
